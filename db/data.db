insert into `html` (`id`, `header`, `title`) VALUES (3, 'Контакты', '<p style="padding-left: 30px;">&nbsp;</p><p style="padding-left: 30px;"><strong>Полное наименование:</strong>&nbsp;Печорская районная общественная организация "Этнокультурное общество народа сето".</p><p style="padding-left: 30px;">&nbsp;</p><p style="padding-left: 30px;"><strong>Место и дата создания:</strong>&nbsp;Псковская область, Печорский район, 1993 г.</p><p style="padding-left: 30px;">&nbsp;</p><p style="padding-left: 30px;"><strong>Председатель:</strong>&nbsp;Маяк Хелья Александровна.&nbsp;</p><p style="padding-left: 30px;">&nbsp;</p><p style="padding-left: 30px;"><strong>Тел.:</strong>&nbsp;8-911-365-56-68, 8-811-48-97-436.&nbsp;</p><p style="padding-left: 30px;">&nbsp;</p><p style="padding-left: 30px;"><strong>Е-mail:&nbsp;</strong> izborsk@yandex.ru</p>');
insert into `html` (`id`, `header`, `title`) VALUES (4, '', '<p style="text-align: center; color: #043d8a; font-weight: bold; text-transform: uppercase;">Последние альбомы:</p><p style="text-align: center;"><a href="album/1"><img src="../../uploads/images/2014/04/12/0_60625400_1397316701_145x145.jpg" alt="" width="145" height="145" /></a></p><p style="text-align: center;">Музей Сето</p><p style="text-align: center;">&nbsp;</p><p style="text-align: center; color: #043d8a; font-weight: bold; text-transform: uppercase;">Наши партнеры:</p><p style="text-align: center;"><img src="http://upload.wikimedia.org/wikipedia/commons/thumb/8/83/Pskovgfull.svg/720px-Pskovgfull.svg.png" alt="" width="150" height="125" /></p><p style="text-align: center;">Администрация г. Псков</p>');

